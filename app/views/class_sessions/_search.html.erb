<h5>Search</h5>
<hr />
<% form_for(@search) do |f| %>
  <p>
    <%= f.label :course_type_id_equals, "Course Type" %><br />
    <%= f.collection_select :course_type_id_equals, CourseType.all , :id, :course_type_name, :include_blank => true %>
  </p>
  <p>
    <%= f.label :class_session_name_like, "Class Session Name" %><br />
    <%= f.text_field :class_session_name_like %>
  </p>
  <p>
    <%= f.label :day_like, "Day" %><br />
    <%= f.select :day_like, @day, :prompt => 'Select Day' %>
  </p>
  <p>
    <%= f.label :start_like, "Start" %><br />
    <%= f.select :start_like, @time.sort %>
  </p>
  <p>
    <%= f.submit 'Search' %>
  </p>
<% end %>