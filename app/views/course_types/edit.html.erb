<h1>Editing Course Type</h1>

<% form_for(@course_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :course_type_name %><br />
    <%= f.text_field :course_type_name %>
  </p>
  <p>
    <%= f.label :course_type_fee %><br />
    <%= f.text_field :course_type_fee %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @course_type %> |
<%= link_to 'Back', course_types_path %>