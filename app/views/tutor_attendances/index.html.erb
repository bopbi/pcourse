<h1>Listing Tutor Attendances</h1>
<%= link_to 'New Tutor Attendance', new_tutor_attendance_path %>
<table>
  <tr>
    <th>Tutor</th>
    <th>Class session</th>
    <th>Attendance day</th>
    <th>Attendance</th>
    <th colspan="3">Action</th>
  </tr>

<% @tutor_attendances.each do |tutor_attendance| %>
  <tr>
    <td><%=h tutor_attendance.tutor_id %></td>
    <td><%=h tutor_attendance.class_session_id %></td>
    <td><%=h tutor_attendance.attendance_day %></td>
    <td><%=h tutor_attendance.attendance %></td>
    <td><%= link_to 'Show', tutor_attendance %></td>
    <td><%= link_to 'Edit', edit_tutor_attendance_path(tutor_attendance) %></td>
    <td><%= link_to 'Destroy', tutor_attendance, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />
<%= will_paginate @tutor_attendances %>
